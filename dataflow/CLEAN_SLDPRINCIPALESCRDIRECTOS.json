{
	"name": "CLEAN_SLDPRINCIPALESCRDIRECTOS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EXCEL_saldosprincipalcreditosdirectos",
						"type": "DatasetReference"
					},
					"name": "EXCELsaldosprincipalcreditosdirectos"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "PARQUET_SLDPRINCIPALESCRDIRECTOS",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "ColumnaFechaProceso"
				}
			],
			"scriptLines": [
				"parameters{",
				"     FechaProc as string",
				"}",
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as double,",
				"          Column_6 as double,",
				"          Column_7 as double,",
				"          Column_8 as double,",
				"          Column_9 as double,",
				"          Column_10 as double,",
				"          Column_11 as double,",
				"          Column_12 as double,",
				"          Column_13 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     preferredFractionalType: 'double') ~> EXCELsaldosprincipalcreditosdirectos",
				"filter1 select(mapColumn(",
				"          Sector = Column_1,",
				"          Entidad = Column_2,",
				"          Actividad = Column_3,",
				"          AlDia = Column_5,",
				"          {30Dias} = Column_6,",
				"          {60Dias} = Column_7,",
				"          {90Dias} = Column_8,",
				"          {180Dias} = Column_9,",
				"          Mas180Dias = Column_10,",
				"          Judicial = Column_11,",
				"          Total = Column_12",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"EXCELsaldosprincipalcreditosdirectos filter(not(equals(Column_3, 'Total'))) ~> filter1",
				"ColumnaFechaProceso keyGenerate(output(key as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"select1 derive(Fecha = $FechaProc) ~> ColumnaFechaProceso",
				"surrogateKey1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     filePattern:'creditosdirectos.parquet',",
				"     truncate: true,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}