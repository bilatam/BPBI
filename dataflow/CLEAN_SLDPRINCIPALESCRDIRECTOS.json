		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EXCEL_saldosprincipalcreditosdirectos",
						"type": "DatasetReference"
					},
					"name": "CSVsaldosprincipalcreditosdirectos"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "PARQUET_SLDPRINCIPALESCRDIRECTOS",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Sector as string,",
				"          Entidad as string,",
				"          Actividad as string,",
				"          Empty as string,",
				"          AlDia as string,",
				"          {30Dias} as string,",
				"          {60Dias} as string,",
				"          {90Dias} as string,",
				"          {180Dias} as string,",
				"          Mas180Dias as string,",
				"          Judicial as string,",
				"          Total as string,",
				"          File as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'FILENAME',",
				"     preferredFractionalType: 'None') ~> CSVsaldosprincipalcreditosdirectos",
				"filter1 select(mapColumn(",
				"          Sector = Empty,",
				"          Entidad = Sector,",
				"          ActividadEconomica = Entidad,",
				"          AlDia = ActividadEconomica,",
				"          {1a30Dias} = Empty2,",
				"          {31a61Dias} = Empty,",
				"          {61a90Dias} = {1a30Dias},",
				"          {91a180Dias} = {31a61Dias},",
				"          Mas180Dias = {61a90Dias},",
				"          CobroJudicial = {91a180Dias},",
				"          Total = {180Dias},",
				"          Column_12 = CobroJudicial,",
				"          Column_13 = Total,",
				"          FILENAME",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"CSVsaldosprincipalcreditosdirectos filter(not(equals(Entidad, 'Total'))) ~> filter1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}